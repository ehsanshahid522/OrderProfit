{
    "name": "orderprofit-monorepo",
    "version": "1.0.0",
    "private": true,
    "scripts": {
        "dev:frontend": "cd frontend && npm run dev",
        "dev:backend": "cd backend && npm run dev",
        "dev": "npm-run-all --parallel dev:frontend dev:backend",
        "install:all": "cd frontend && npm install && cd ../backend && npm install"
    },
    "dependencies": {
        "bcryptjs": "^3.0.3",
        "cors": "^2.8.5",
        "dotenv": "^17.2.3",
        "express": "^5.2.1",
        "jsonwebtoken": "^9.0.3",
        "mongoose": "^9.0.2"
    },
    "devDependencies": {
        "npm-run-all": "^4.1.5"
    }
}